<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--#region Colors-->
    <Color x:Key="Gray050">#A50E0E0E</Color>
    <Color x:Key="Gray100">#FF919191</Color>
    <Color x:Key="Gray200">#FF6E6E6E</Color>
    <Color x:Key="Gray300">#FF404040</Color>
    <Color x:Key="Gray400">#FF212121</Color>
    <Color x:Key="Red100">#FFFA0000</Color>
    <Color x:Key="Red200">#FFE50000</Color>
    <Color x:Key="Red300">#FFB10000</Color>
    <Color x:Key="Red400">#FF780202</Color>
    <Color x:Key="Blue100">#FF00A8E8</Color>
    <Color x:Key="Blue200">#FF007EA7</Color>
    <Color x:Key="Blue300">#FF003459</Color>
    <Color x:Key="Blue400">#FF00171F</Color>
    <Color x:Key="Green100">#FFB6FF87</Color>
    <Color x:Key="Green200">#FF0ECC32</Color>
    <Color x:Key="Green300">#FF027825</Color>
    <Color x:Key="Green400">#FF002B00</Color>
    <Color x:Key="Gold100">#FFFFF4D3</Color>
    <Color x:Key="Gold200">#FFFFEBAA</Color>
    <Color x:Key="Gold300">#FFFFCE2A</Color>
    <Color x:Key="Gold400">#FFE5A504</Color>
    <Color x:Key="Purple100">#FF512BD4</Color>
    <Color x:Key="Purple200">#FF52528C</Color>
    <Color x:Key="Purple300">#FF372554</Color>
    <Color x:Key="Purple400">#FF231123</Color>
    <!--#endregion-->
    <!--#region Brushes-->
    <SolidColorBrush x:Key="Gray050Brush" Color="{StaticResource Gray050}"/>
    <SolidColorBrush x:Key="Gray100Brush" Color="{StaticResource Gray100}"/>
    <SolidColorBrush x:Key="Gray200Brush" Color="{StaticResource Gray200}"/>
    <SolidColorBrush x:Key="Gray300Brush" Color="{StaticResource Gray300}"/>
    <SolidColorBrush x:Key="Gray400Brush" Color="{StaticResource Gray400}"/>
    <SolidColorBrush x:Key="Red100Brush" Color="{StaticResource Red100}"/>
    <SolidColorBrush x:Key="Red200Brush" Color="{StaticResource Red200}"/>
    <SolidColorBrush x:Key="Red300Brush" Color="{StaticResource Red300}"/>
    <SolidColorBrush x:Key="Red400Brush" Color="{StaticResource Red400}"/>
    <SolidColorBrush x:Key="Blue100Brush" Color="{StaticResource Blue100}"/>
    <SolidColorBrush x:Key="Blue200Brush" Color="{StaticResource Blue200}"/>
    <SolidColorBrush x:Key="Blue300Brush" Color="{StaticResource Blue300}"/>
    <SolidColorBrush x:Key="Blue400Brush" Color="{StaticResource Blue400}"/>
    <SolidColorBrush x:Key="Green100Brush" Color="{StaticResource Green100}"/>
    <SolidColorBrush x:Key="Green200Brush" Color="{StaticResource Green200}"/>
    <SolidColorBrush x:Key="Green300Brush" Color="{StaticResource Green300}"/>
    <SolidColorBrush x:Key="Green400Brush" Color="{StaticResource Green400}"/>
    <SolidColorBrush x:Key="Gold100Brush" Color="{StaticResource Gold100}"/>
    <SolidColorBrush x:Key="Gold200Brush" Color="{StaticResource Gold200}"/>
    <SolidColorBrush x:Key="Gold300Brush" Color="{StaticResource Gold300}"/>
    <SolidColorBrush x:Key="Gold400Brush" Color="{StaticResource Gold400}"/>
    <SolidColorBrush x:Key="Purple100Brush" Color="{StaticResource Purple100}"/>
    <SolidColorBrush x:Key="Purple200Brush" Color="{StaticResource Purple200}"/>
    <SolidColorBrush x:Key="Purple300Brush" Color="{StaticResource Purple300}"/>
    <SolidColorBrush x:Key="Purple400Brush" Color="{StaticResource Purple400}"/>
    <!--#endregion-->
    <!--#region Linear Gradient Brushes-->
    <LinearGradientBrush x:Key="LBrGray" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="{StaticResource Gray050}" Offset="0"/>
        <GradientStop Color="{StaticResource Gray300}" Offset="1"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="LBrRed" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="{StaticResource Red100}" Offset="0"/>
        <GradientStop Color="{StaticResource Red300}" Offset="1"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="LBrBlue" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="{StaticResource Blue100}" Offset="0"/>
        <GradientStop Color="{StaticResource Blue300}" Offset="1"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="LBrGreen" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="{StaticResource Green100}" Offset="0"/>
        <GradientStop Color="{StaticResource Green300}" Offset="1"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="LBrGold" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="{StaticResource Gold100}" Offset="0"/>
        <GradientStop Color="{StaticResource Gold300}" Offset="1"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="LBrPurple" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="{StaticResource Purple100}" Offset="0"/>
        <GradientStop Color="{StaticResource Purple300}" Offset="1"/>
    </LinearGradientBrush>
    <!--#endregion-->
</ResourceDictionary>